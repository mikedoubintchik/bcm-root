    "angular-ui-router": "^1.0.26",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-config-important-stuff": "^1.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "jest-cli": "^25.4.0",
